{
	"name": "elvis/atomino",
	"description": "atomino project",
	"authors": [
		{
			"name": "laborci",
			"email": "gergely@laborci.hu"
		}
	],
	"repositories": [
		{
			"type": "path",
			"url": "~/Work/composer/atomino-framework/*"
		}
	],
	"require": {
		"atomino/core": "dev-master",
		"atomino/magic": "dev-master",
		"atomino/plugin-created": "dev-master",
		"atomino/plugin-updated": "dev-master",
		"atomino/plugin-guid": "dev-master",
		"atomino/plugin-attachment": "dev-master",
		"atomino/plugin-authenticate": "dev-master",
		"atomino/plugin-authorize": "dev-master",
		"atomino/plugin-comment": "dev-master",
		"ext-json": "*",
		"ext-gd": "*",
		"ext-curl": "*"
	},
	"autoload": {
		"psr-4": {
			"Atomino\\Atoms\\": "dev/atoms",
			"Application\\": "dev/src",
			"Application\\Web\\": "dev/src/@Web",
			"Application\\Api\\": "dev/src/@Api",
			"Application\\Admin\\": "dev/src/@Admin"
		}
	}
}
