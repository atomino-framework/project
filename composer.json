{
	"name": "atomino/project",
	"description": "atomino project",
	"authors": [
		{
			"name": "Gergely Laborci",
			"email": "gergely@laborci.hu"
		},
		{
			"name": "Balázs Láng",
			"email": "balazs.lang@outlook.de"
		}
	],
	"scripts": {
		"post-root-package-install": [
			"php post-install-script.php"
		],
		"post-create-project-cmd": [
			"rmdir .my-repos"
		]
	},
	"repositories": [
		{
			"type": "path",
			"url": ".my-repos/*"
		}
	],
	"require": {
		"atomino/core": "dev-master",
		"atomino/magic": "dev-master",
		"atomino/bundle-created": "dev-master",
		"atomino/bundle-updated": "dev-master",
		"atomino/bundle-guid": "dev-master",
		"atomino/bundle-authenticate": "dev-master",
		"atomino/bundle-authorize": "dev-master",
		"atomino/bundle-comment": "dev-master",
		"atomino/bundle-attachment": "dev-master",
		"atomino/bundle-json": "dev-master",
		"atomino/bundle-rlogtail": "dev-master",
		"atomino/carbon": "dev-master",
		"atomino/mercury": "dev-master",
		"ext-json": "*",
		"ext-gd": "*",
		"ext-curl": "*"
	},
	"autoload": {
		"psr-4": {
			"Atomino\\Atoms\\": "dev/atoms",
			"Application\\": "dev/src",
			"Application\\Web\\": "dev/src/@Web",
			"Application\\Api\\": "dev/src/@Api",
			"Application\\Admin\\": "dev/src/@Admin"
		}
	}
}
